	"derivation": "constraint",
	"differential": {
		"element": [
			{
				"id": "Observation",
				"path": "Observation",
				"definition": "This profile is  created to meet the 2015 Edition Common Clinical Data Set 'Laboratory test(s) and Laboratory value(s)/result(s)' requirements.",
				"constraint": [
					{
						"key": "us-core-2",
						"severity": "error",
						"human": "If there is no component or related element then either a value[x] or a data absent reason must be present",
						"expression": "(component.empty() and related.empty()) implies (dataAbsentReason or value)",
						"xpath": "exists(f:component) or exists(f:related) or exists(f:*[starts-with(local-name(.), 'value)]) or exists(f:dataAbsentReason)"
					}
				],
				"mustSupport": false,
				"mapping": [
					{
						"identity": "argonaut-dq-dstu2",
						"map": "Observation"
					}
				]
			},
			{
				"id": "Observation.status",
				"path": "Observation.status",
				"min": 1,
				"max": "1",
				"type": [
					{
						"code": "code"
					}
				],
				"mustSupport": true,
				"binding": {
					"strength": "required",
					"valueSet": "http://hl7.org/fhir/ValueSet/observation-status"
				},
				"mapping": [
					{
						"identity": "argonaut-dq-dstu2",
						"map": "Observation.status"
					}
				]
			},
